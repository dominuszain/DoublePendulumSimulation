Cell["\<\
These above differential equations are coupled. We need to separate the \
t1dotdot and t2dotdot into two separate differential equations. For that, we \
would simply solve these simultaneously.\
\>", "Text",
 CellChangeTimes->{{3.9021263648947725`*^9, 
  3.902126409839814*^9}},ExpressionUUID->"ab3a82c2-3598-4fd2-8dd8-\
b5d03a600f22"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["l1", "2"], " ", "m1", " ", "t1dotdot"}], "+", 
       RowBox[{
        SuperscriptBox["l1", "2"], " ", "m2", " ", "t1dotdot"}], "+", 
       RowBox[{"l1", " ", "l2", " ", "m2", " ", "t2dotdot", " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"t1", "-", "t2"}], "]"}]}], "+", 
       RowBox[{"g", " ", "l1", " ", "m1", " ", 
        RowBox[{"Sin", "[", "t1", "]"}]}], "+", 
       RowBox[{"g", " ", "l1", " ", "m2", " ", 
        RowBox[{"Sin", "[", "t1", "]"}]}], "+", 
       RowBox[{"l1", " ", "l2", " ", "m2", " ", 
        SuperscriptBox["t2dot", "2"], " ", 
        RowBox[{"Sin", "[", 
         RowBox[{"t1", "-", "t2"}], "]"}]}]}], "\[Equal]", "0"}], ",", " ", 
     RowBox[{
      RowBox[{"l2", " ", "m2", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"l2", " ", "t2dotdot"}], "+", 
         RowBox[{"l1", " ", "t1dotdot", " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"t1", "-", "t2"}], "]"}]}], "-", 
         RowBox[{"l1", " ", 
          SuperscriptBox["t1dot", "2"], " ", 
          RowBox[{"Sin", "[", 
           RowBox[{"t1", "-", "t2"}], "]"}]}], "+", 
         RowBox[{"g", " ", 
          RowBox[{"Sin", "[", "t2", "]"}]}]}], ")"}]}], "\[Equal]", "0"}]}], 
    "}"}], ",", " ", 
   RowBox[{"{", 
    RowBox[{"t1dotdot", ",", " ", "t2dotdot"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.9021264141196833`*^9, 3.902126463834098*^9}},
 CellLabel->"In[38]:=",ExpressionUUID->"0e69bc64-0a43-463a-9890-5fa92bb671fe"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"t1dotdot", "\[Rule]", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{
        RowBox[{"g", " ", "m1", " ", 
         RowBox[{"Sin", "[", "t1", "]"}]}], "+", 
        RowBox[{"g", " ", "m2", " ", 
         RowBox[{"Sin", "[", "t1", "]"}]}], "+", 
        RowBox[{"l2", " ", "m2", " ", 
         SuperscriptBox["t2dot", "2"], " ", 
         RowBox[{"Sin", "[", 
          RowBox[{"t1", "-", "t2"}], "]"}]}], "+", 
        RowBox[{"l1", " ", "m2", " ", 
         SuperscriptBox["t1dot", "2"], " ", 
         RowBox[{"Cos", "[", 
          RowBox[{"t1", "-", "t2"}], "]"}], " ", 
         RowBox[{"Sin", "[", 
          RowBox[{"t1", "-", "t2"}], "]"}]}], "-", 
        RowBox[{"g", " ", "m2", " ", 
         RowBox[{"Cos", "[", 
          RowBox[{"t1", "-", "t2"}], "]"}], " ", 
         RowBox[{"Sin", "[", "t2", "]"}]}]}], 
       RowBox[{"l1", " ", 
        RowBox[{"(", 
         RowBox[{"m1", "+", "m2", "-", 
          RowBox[{"m2", " ", 
           SuperscriptBox[
            RowBox[{"Cos", "[", 
             RowBox[{"t1", "-", "t2"}], "]"}], "2"]}]}], ")"}]}]]}]}], ",", 
    RowBox[{"t2dotdot", "\[Rule]", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"l2", " ", 
        RowBox[{"(", 
         RowBox[{"m1", "+", "m2", "-", 
          RowBox[{"m2", " ", 
           SuperscriptBox[
            RowBox[{"Cos", "[", 
             RowBox[{"t1", "-", "t2"}], "]"}], "2"]}]}], ")"}]}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"g", " ", "m1", " ", 
         RowBox[{"Cos", "[", 
          RowBox[{"t1", "-", "t2"}], "]"}], " ", 
         RowBox[{"Sin", "[", "t1", "]"}]}], "+", 
        RowBox[{"g", " ", "m2", " ", 
         RowBox[{"Cos", "[", 
          RowBox[{"t1", "-", "t2"}], "]"}], " ", 
         RowBox[{"Sin", "[", "t1", "]"}]}], "+", 
        RowBox[{"l1", " ", "m1", " ", 
         SuperscriptBox["t1dot", "2"], " ", 
         RowBox[{"Sin", "[", 
          RowBox[{"t1", "-", "t2"}], "]"}]}], "+", 
        RowBox[{"l1", " ", "m2", " ", 
         SuperscriptBox["t1dot", "2"], " ", 
         RowBox[{"Sin", "[", 
          RowBox[{"t1", "-", "t2"}], "]"}]}], "+", 
        RowBox[{"l2", " ", "m2", " ", 
         SuperscriptBox["t2dot", "2"], " ", 
         RowBox[{"Cos", "[", 
          RowBox[{"t1", "-", "t2"}], "]"}], " ", 
         RowBox[{"Sin", "[", 
          RowBox[{"t1", "-", "t2"}], "]"}]}], "-", 
        RowBox[{"g", " ", "m1", " ", 
         RowBox[{"Sin", "[", "t2", "]"}]}], "-", 
        RowBox[{"g", " ", "m2", " ", 
         RowBox[{"Sin", "[", "t2", "]"}]}]}], ")"}]}]}]}], "}"}], 
  "}"}]], "Output",
 CellChangeTimes->{3.90212646624234*^9, 3.902126699932911*^9},
